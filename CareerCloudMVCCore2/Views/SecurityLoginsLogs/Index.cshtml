@model IEnumerable<CareerCloudMVCCore2.Models.SecurityLoginsLog>

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.SourceIp)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.LogonDate)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.IsSuccesful)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.LoginNavigation)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.SourceIp)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.LogonDate)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.IsSuccesful)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.LoginNavigation.EmailAddress)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.Id">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.Id">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.Id">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
